<gconfschemafile>
    <schemalist>
      <schema>
        <key>/schemas/apps/inkblot/bus</key>
        <applyto>/apps/inkblot/bus</applyto>
        <owner>inkblot</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
        	<short>libinklevel bus type</short>
	        <long>
		1 = Parallel Port, 2 = USB
		</long>
        </locale>
      </schema>
	<schema>
        <key>/schemas/apps/inkblot/port</key>
        <applyto>/apps/inkblot/port</applyto>
        <owner>inkblot</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
                <short>libinklevel port number</short>
                <long>
		the number of the lp port - ie. 0 = lp0
                </long>
        </locale>
      </schema>
	<schema>
        <key>/schemas/apps/inkblot/first-run</key>
        <applyto>/apps/inkblot/first-run</applyto>
        <owner>inkblot</owner>
        <type>int</type>
        <default>1</default>
        <locale name="C">
                <short>libinklevel config indicator</short>
                <long>
                If this key is set, inkblot will try to find new devices and will set up the other keys.
                </long>
        </locale>
      </schema>
     </schemalist>
</gconfschemafile>
  
